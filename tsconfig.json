{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "DOM",
      "ESNext",
    ],
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": false,
    "checkJs": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "static/js/node",
    "types": [
      "node",
    ]
  }
}